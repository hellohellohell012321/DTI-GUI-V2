    clearFace()

    -- symbol

    applyDecalList({
        122736952403507,
        99296401621511,
        104182823148000,
        101540726874910,
        116134771181973
    })

    wait(1)

end
end)

closeButton.MouseButton1Click:Connect(function()
    screengui:Destroy()
end)
