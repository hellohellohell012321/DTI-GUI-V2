local NotificationLibrary =
    loadstring(game:HttpGet("https://raw.githubusercontent.com/hellohellohell012321/TALENTLESS/main/notif_lib.lua"))(
)

local function playSound(soundId, loudness)
    local sound = Instance.new("Sound")
    sound.SoundId = "rbxassetid://" .. soundId
    sound.Parent = game.Players.LocalPlayer.Character or game.Players.LocalPlayer
    sound.Volume = loudness or 1
    sound:Play()
end

-- drag

local UserInputService = game:GetService("UserInputService")

local gui = frame

local dragging
local dragInput
local dragStart
local startPos
	
local function update(input)
	local delta = input.Position - dragStart
	gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
gui.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = gui.Position
	
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)
	
gui.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)
	
UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
end
end)



local function applyDecalFace(type, decals, layer)
    local function resolveImageId(id)
        local success, obj = pcall(function()
            return game:GetObjects("rbxassetid://" .. tostring(id))[1]
        end)

        if success and obj then
            return obj.Texture -- Returns the real image ID
        else
            -- Assume it's already the image ID
            return "rbxassetid://" .. tostring(id)
        end
    end

    local finalImage
    if typeof(decals) == "table" then
        finalImage = {}
        for i, id in ipairs(decals) do
            finalImage[i] = resolveImageId(id)
        end
    else
        finalImage = resolveImageId(decals)
    end

    print("Image(s) to use:", finalImage)

    game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("CustomMakeup"):FireServer(
        type, 
        finalImage,
        tostring(layer)
    )
end



local makeUps = {

    {name = "Contour", layer = ""},
    {name = "Touches", layer = ""},
    {name = "Touches", layer = 2},
    {name = "Touches", layer = 3},
    {name = "Touches", layer = 4},
    {name = "Touches", layer = 5},
    {name = "Touches", layer = 6},
    {name = "Eyes", layer = ""},
    {name = "Lips", layer = ""},
    {name = "Eyes", layer = 2},
    {name = "Lips", layer = 2},
    {name = "Eyebrows", layer = ""},
    {name = "Lips", layer = 3},
}



local function clearFace()
    local ids = {"", "", "", "", "", "", "", "", "", "", "", "", ""}
    if #ids >= 14 then print("Too many IDs") return end

    for i = 1, #ids do
    local decal
    if i == 10 then 
        decal = {ids[8], ids[10]} elseif
    i == 11 then
        decal = {ids[9], ids[11]} elseif
    i == 13 then
        decal = {ids[9], ids[11], ids[13]} else
    decal = ids[i] end


    local current = makeUps[i]

    applyDecalFace(current.name, decal, current.layer)
end

end



local function applyDecalList(ids)
    if #ids >= 14 then print("Too many IDs") return end

    for i = 1, #ids do
    local decal
    if i == 10 then 
        decal = {ids[8], ids[10]} elseif
    i == 11 then
        decal = {ids[9], ids[11]} elseif
    i == 13 then
        decal = {ids[9], ids[11], ids[13]} else
    decal = ids[i] end

    local current = makeUps[i]

    applyDecalFace(current.name, decal, current.layer)

    end

end



local function duringrunway()
    for _, descendant in ipairs(game:GetDescendants()) do
        if descendant:IsA("Sound") and descendant.SoundId == "rbxassetid://17208533788" and descendant.IsPlaying then
            return true, descendant
        end
    end
    return false, nil
end



function keepCharacterUpright()
    local character = game.Players.LocalPlayer.Character
    if character then
        local rootPart = character:FindFirstChild("HumanoidRootPart")
        if rootPart then
            local position = rootPart.Position
            local forward = rootPart.CFrame.LookVector
            rootPart.CFrame = CFrame.new(position, position + Vector3.new(forward.X, 0, forward.Z))
        end
    end
end



local button = Instance.new("TextButton")
    button.Parent = scroll
    button.BorderSizePixel = 4
    button.TextColor3 = Color3.fromRGB(113, 113, 113)
    button.BackgroundColor3 = Color3.fromRGB(242, 242, 242)
    button.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json", Enum.FontWeight.Bold, Enum.FontStyle.Normal)
    button.Size = UDim2.new(0, 195, 0, 35)
    button.BorderColor3 = Color3.fromRGB(255, 210, 221)
    button.TextSize = 25
    button.Text = "shove face"

    button.MouseButton1Click:Connect(function()
    niggers = not niggers

    local Players = game:GetService("Players")
    local RunService = game:GetService("RunService")
    local player = Players.LocalPlayer
    local camera = workspace.Locators.Runway.Camera3

    local offset = Vector3.new(0.8, -1.6, 0)

    local function teleportToCamera()
        local character = player.Character
        if character then
            local targetPart =
                character:FindFirstChild("HumanoidRootPart") or character:FindFirstChildWhichIsA("BasePart")
            if targetPart then
                targetPart.CFrame = CFrame.new(camera.CFrame.Position + offset)
            end

            local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                hrp.CFrame = CFrame.lookAt(hrp.Position, camera.CFrame.Position)
            end
            keepCharacterUpright()
        end
    end

    if not duringrunway() then
        playSound("6493287948", 0.1)
        NotificationLibrary:SendNotification(
            "Error",
            "This can only be used during the runway/fashion show.",
            5
        )
        return
    end

    player.Character.Head:Destroy()

    playSound("6493287948", 0.1)
    NotificationLibrary:SendNotification("Success", "Wait 10 seconds for this to work.", 5)

    while niggers do
        if duringrunway() then
            teleportToCamera()
        else
            niggers = false
        end
        task.wait()
    end
end)

